{
  "metadata": [
    {
      "src": [
        {
          "files": ["**/*.csproj"],
          "exclude": [ "**/bin/**", "**/obj/**" ], // `exclude` is also relative to `cwd`
          "cwd": "../src"
        }
      ],
      "dest": "obj/docfx/api/dotnet"
    },
    {
      "src": [
        {
          "files": ["**/*.js"],
          "cwd": "../src"
        }
      ],
      "dest": "obj/docfx/api/js" // throw error when dest is not unique
    }
  ],
  "build": { // input could be YAML or MARKDOWN files, outputs are Final-YAML files
    "content":
      [
        {
          "files": ["**/*.yml"],
          "cwd": "obj/docfx"
        },
        {
          "files": ["tutorial/**/*.md", "spec/**/*.md"]
        },
        {
          "files": ["toc.yml"]
        }
      ],
    "resource": [
        {
          "files": ["images/**"]
        }
    ],
    "overwrite": "apispec/*.md",
    "externalReference": [
      "external/*.yml.zip"
    ],
    "dest": "_site",
    "template": "default",
    "theme": "happy",
    "title": "Doc-as-code documentation"
  }
}